{
  "name": "@wmm387/unocss-preset",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@10.2.1",
  "description": "Unocss Preset For Myself Use",
  "author": "Zero (https://github.com/wmm387)",
  "license": "MIT",
  "homepage": "https://github.com/wmm387/unocss-preset",
  "keywords": [
    "css",
    "unocss",
    "unocss-preset"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsx scripts/build.ts",
    "release": "bumpp --no-push",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test-build": "pnpm build && vitest --mode test-build",
    "test-build:ui": "pnpm build && vitest --mode test-build --ui",
    "lint": "eslint ./src",
    "up": "taze major -I"
  },
  "peerDependencies": {
    "unocss": ">= 65.4.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.17.17",
    "@types/postcss-js": "^4.0.4",
    "@unocss/autocomplete": "^65.4.3",
    "@vitest/ui": "^3.0.5",
    "bumpp": "^10.0.2",
    "eslint": "^9.19.0",
    "fs-extra": "^11.3.0",
    "pnpm": "^10.2.1",
    "postcss": "^8.5.1",
    "postcss-js": "^4.0.1",
    "rollup": "^4.34.4",
    "rollup-plugin-dts": "^6.1.1",
    "taze": "^18.4.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unocss": "^65.4.3",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  }
}
